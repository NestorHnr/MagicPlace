

<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

@if(TempData["Exitoso"]  != null)
{
    <script>
        Swal.fire({
            position: 'top-end',
            icon: 'success',
            title: '@TempData["Exitoso"]',
            showConfirmButton: false,
            timer: 1500
        })
    </script>
}

@if (TempData["Error"] != null)
{
    <script>
        Swal.fire({
            position: 'top-end',
            icon: 'error',
            title: '@TempData["Error"]',
            showConfirmButton: false,
            timer: 1500
        })
    </script>
}